			android:layout_marginStart="25.4dp"
			android:background="#e3e3e3" />
		
		<View
			android:id="@+id/view3"
			android:layout_width="3.1dp"
			android:layout_height="3dp"
			android:layout_below="@+id/view"
			android:layout_alignParentEnd="true"
			android:layout_marginEnd="25.4dp"
			android:background="#e3e3e3" />
		
		<View
			android:id="@+id/view4"
			android:layout_width="3.1dp"
			android:layout_height="3dp"
			android:layout_below="@+id/view2"
			android:layout_marginStart="22.3dp"
			android:background="#e3e3e3" />
		
		<View
			android:id="@+id/view5"
			android:layout_width="3.1dp"
			android:layout_height="3dp"
			android:layout_below="@+id/view2"
			android:layout_alignParentEnd="true"
			android:layout_marginEnd="22.3dp"
			android:background="#e3e3e3" />
		
		<View
			android:id="@+id/view6"
			android:layout_width="match_parent"
			android:layout_height="5dp"
			android:layout_below="@+id/view2"
			android:layout_marginStart="33.8dp"
			android:layout_marginTop="1dp"
			android:layout_marginEnd="33.8dp"
			android:background="#e3e3e3" />
		
		<View
			android:id="@+id/view7"
			android:layout_width="3.1dp"
			android:layout_height="3dp"
			android:layout_below="@+id/view4"
			android:layout_marginStart="19.1dp"
			android:background="#e3e3e3" />
		
		<View
			android:id="@+id/view8"
			android:layout_width="3.1dp"
			android:layout_height="3dp"
			android:layout_below="@+id/view4"
			android:layout_alignParentEnd="true"
			android:layout_marginEnd="19.1dp"
			android:background="#e3e3e3" />
		
		<View
			android:id="@+id/view9"
			android:layout_width="5.2dp"
			android:layout_height="5dp"
