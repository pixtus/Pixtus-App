		<TextView
			android:id="@+id/story_text"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:text="사막에 픽터스라는 외로운 선인장이 살았다."
			android:textColor="#525252"
			android:fontFamily="@font/notosans_regular"
			android:layout_below="@id/view6"
			android:layout_marginStart="40dp"
			android:layout_marginEnd="40dp"
			android:layout_marginTop="12dp"/>
		
		<ImageView
			android:id="@+id/story_next_btn"
			android:layout_width="30dp"
			android:layout_height="30dp"
			android:layout_above="@id/view20"
			android:layout_alignParentEnd="true"
			android:layout_marginEnd="34dp"
			android:layout_marginBottom="6dp"
			app:srcCompat="@mipmap/btn_workout" />
		
	</RelativeLayout>
	
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="20dp">
		
		<TextView
			android:id="@+id/skip_btn"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			android:text="SKIP"
			android:textSize="14sp"
			android:fontFamily="@font/notosans_regular"
			android:fontColor="#c4c4c4"/>
		
		<View
			android:layout_width="130dp"
			android:layout_height="1.5dp"
			android:layout_marginRight="11dp"
			android:layout_toLeftOf="@id/skip_btn"
			android:layout_centerVertical="true"
			android:background="#e3e3e3" />
		
		<View
			android:layout_width="130dp"
			android:layout_height="1.5dp"
			android:layout_marginStart="11dp"
			android:layout_toRightOf="@id/skip_btn"
			android:layout_centerVertical="true"
			android:background="#e3e3e3" />
